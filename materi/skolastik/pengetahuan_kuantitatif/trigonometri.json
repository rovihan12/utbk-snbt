{
  "judul": "Trigonometri",
  "konten": [
    {
      "tipe": "heading",
      "teks": "Pengertian Trigonometri"
    },
    {
      "tipe": "paragraf",
      "teks": "Trigonometri adalah cabang matematika yang mempelajari hubungan antara sudut dan sisi segitiga, khususnya segitiga siku-siku."
    },
    {
      "tipe": "heading",
      "teks": "Perbandingan Trigonometri Dasar"
    },
    {
      "tipe": "rumus",
      "teks": "sin θ = sisi depan / sisi miring"
    },
    {
      "tipe": "rumus",
      "teks": "cos θ = sisi samping / sisi miring"
    },
    {
      "tipe": "rumus",
      "teks": "tan θ = sisi depan / sisi samping"
    },
    {
      "tipe": "heading",
      "teks": "Identitas Trigonometri Penting"
    },
    {
      "tipe": "rumus",
      "teks": "sin²θ + cos²θ = 1"
    },
    {
      "tipe": "rumus",
      "teks": "1 + tan²θ = sec²θ"
    },
    {
      "tipe": "rumus",
      "teks": "1 + cot²θ = csc²θ"
    }
  ]
}
